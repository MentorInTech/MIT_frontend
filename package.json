{
  "name": "mit_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "material-ui": "^0.20.0",
    "material-ui-places": "^1.1.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-geosuggest": "^2.7.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-validation-framework": "^4.2.0",
    "redux": "^3.0.0",
    "redux-form-material-ui": "^4.3.3"
  },
  "scripts": {
    "start": "PORT=8080 react-scripts start",
    "build": "npm test && react-scripts build",
    "clean": "rm -rf node_modules build coverage",
    "test": "npm run lint && CI=true react-scripts test --env=jsdom --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix ; npm run lint:css:fix",
    "lint:js": "eslint --ext js --ext jsx src",
    "lint:js:fix": "eslint --fix --ext js --ext jsx src",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "stylelint --fix src/**/*.css"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.1.0"
  }
}
