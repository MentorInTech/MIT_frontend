{
  "name": "mit_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm test && react-scripts build",
    "test": "npm run lint && CI=true react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext js --ext jsx src",
    "lint:js:fix": "eslint --fix --ext js --ext jsx src",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "stylelint --fix src/**/*.css"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  }
}
